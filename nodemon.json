{
  "watch": [
    "src",
    "prisma",
    "proxy.ts",
    "next.config.ts"
  ],
  "ext": "ts,tsx,js,jsx,json,prisma",
  "ignore": [
    "node_modules",
    ".next",
    "coverage",
    "*.test.ts",
    "*.test.tsx",
    "*.spec.ts",
    "*.spec.tsx"
  ],
  "exec": "next dev",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true
}

